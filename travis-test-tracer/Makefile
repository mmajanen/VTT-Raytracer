# GNU Make solution makefile autogenerated by Premake
# Type "make help" for usage help

ifndef config
  config=debug
endif
export config

PROJECTS := tracer

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

tracer: 
	@echo "==== Building tracer ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f tracer.make

clean:
	@${MAKE} --no-print-directory -C . -f tracer.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug"
	@echo "   release"
	@echo "   debug_verbose"
	@echo "   releasenorays"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   tracer"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
